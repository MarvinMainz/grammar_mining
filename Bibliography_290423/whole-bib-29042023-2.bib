@article{ramirezKoropoPuriKamaka2015,
  title = {{Korop\'o, puri, kamak\~a e outras l\'inguas do Leste Brasileiro}},
  author = {Ramirez, Henri and Vegini, Valdir and de Fran{\c c}a, Maria Cristina Victorino},
  year = {2015},
  month = sep,
  journal = {LIAMES: L\'inguas Ind\'igenas Americanas},
  volume = {15},
  number = {2},
  pages = {223--277},
  issn = {2177-7160},
  doi = {10.20396/liames.v15i2.8642302},
  urldate = {2023-04-11},
  abstract = {Neste estudo, realizamos uma an\'alise aprofundada de l\'inguas extintas localizadas no Leste do Brasil (de S\~ao Paulo a Salvador). Tradicionalmente, agrupa-se o korop\'o com o puri (coroado) e ambos na superfam\'ilia macro-j\^e. Inicialmente, refutamos qualquer afinidade entre as l\'inguas korop\'o e puri e, subsequentemente, apresentamos raz\~oes para excluir o puri-coroado das l\'inguas macro-j\^e. Prosseguindo em dire\c{c}\~ao ao norte, examinamos detalhadamente a fam\'ilia maxakali, propomos uma classifica\c{c}\~ao provis\'oria de suas l\'inguas afiliadas e comparamos pormenorizadamente essa fam\'ilia com l\'inguas do seu entorno geogr\'afico (kamak\~a, j\^e e krenak). Desse estudo, podemos concluir que o maxakali mant\'em estreita rela\c{c}\~ao com o kamak\~a, sugerindo-nos a exist\^encia de um v\'inculo gen\'etico entre esses dois grupos lingu\'isticos. Contudo, n\~ao podemos propor rela\c{c}\~ao gen\'etica semelhante para todas as quatro fam\'ilias (maxakali, kamak\~a, j\^e, krenak) visto que seus falantes viviam numa \'area onde houve um longo per\'iodo de contatos inter\'etnicos que poderia ter favorecido o compartilhamento de grande n\'umero de empr\'estimos lingu\'isticos entre esses idiomas.},
  langid = {portuguese},
  keywords = {coro1248,grammar\_sketch,Korop\'o,MED,MRD,nucl1710,puri,puri1261,puri1262},
  file = {files/3024/Ramirez et al_2015_Koropó, puri, kamakã e outras línguas do Leste Brasileiro.pdf}
}

